{
  "name": "chrome-element-screenshot",
  "version": "1.0.0",
  "description": "Chrome extension for taking screenshots of specific elements",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.0.0",
    "jsdom": "^26.1.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  }
}
